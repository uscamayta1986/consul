<div id="public_interests" class="public-interests">
  <h4><%= interests_title_text(user) %></h4>

  <% if user.interests.any? %>
    <ul class="no-bullet tags">
      <% user.interests.each do |interest| %>
        <li class="inline-block"><span><%= interest %></span></li>
      <% end %>
    </ul>
  <% end %>
</div>
